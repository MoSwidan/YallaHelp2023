@page "/Contact-US"
<Header />
 <!-- ======== main started ========= -->

<main class="charity-01-main " style="margin-top:102px;">

    <!-- ============abt-01 Section  Start============ -->
    <BreadCrumb BaseRouteName="الرئيسيه" BaseRouteUrl="/" SubRouteName="تواصل معنا" />

    <section class="bg-0-b" dir="rtl">
        <div class="container">
            <div class="row text-right">
                <div class="main-card-contact d-flex">
                    <div class="sup-card-contact">
                        <div class="sup-content">
                            <div class="head-content">
                                <h2>أترك رسالتك هنا</h2>
                                <p>
                                   سعداء دائما بتلقي شكاوي واقتراحات المستخدمين لتحسين وتطوير الخدمة المقدمة للوصول لاعلي درجات رضاء المستخدم وأفضل تجربة استخدام للمنصه.
                                </p>
                            </div>

                            <div class="contact-title">
                                <h2>بيانات التواصل</h2>
                                <ol>
                                    <li><i class="far fa-map-marker-check pr-0 pl-1"></i>القاهرة</li>
                                    <li><i class="fal fa-mobile pr-0 pl-1"></i>201124651160+</li>
                                    <li><i class="fal fa-envelope pr-0 pl-1"></i>Support@yallahelp.com</li>
                                    <li><i class="fal fa-clock pr-0 pl-1"></i>
                                        السبت - الخميس - 8.00ص - 10.00م</li>
                                </ol>
                            </div>
                        </div>
                    </div>

                    <div class="sup-card-contact-0a">
                        <div class="contact-a1">
                            <form>
                                <div class="dived d-flex">
                                    <div class="form-group mb-0">
                                        <div class="form-sup">
                                            <div class="cal-01 ">
                                                <input type="name" name="name" id="" class="form-control  text-center"
                                                       placeholder="الاسم">
                                                <i class="fal fa-user-tie"></i>
                                            </div>

                                            <div class="cal-01">
                                                <input type="phone" name="phone" id="" class="form-control  text-center"
                                                       placeholder="رقم الهاتف">
                                                <i class="fal fa-phone"></i>
                                            </div>
                                            <div class="cal-01">
                                                <input type="email" name="email" id="" class="form-control text-center"
                                                       placeholder="الايميل">
                                                <i class="fal fa-at"></i>
                                            </div>
                                            <div class="cal-01">
                                                <input type="text" name="subject" id="" class="form-control text-center"
                                                       placeholder="الموضوع">
                                                <i class="fal fa-envelope-open-text"></i>
                                            </div>
                                        </div>
                                    </div>

                                    

                                    <div class="ca-ool">
                                        <textarea name="text" cols="80" rows="6" class="form-control"
                                                  placeholder="اكتب رسالتك هنا"></textarea>
                                    </div>
                                    <div class="col-12 mt-4">
                                        <div class="col-3 m-auto p-2 text-white text-center rounded" style="background:#fd1b5b;">
                                            أرسل
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="mab-01">
        <iframe style="width:100%"
                src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d249759.19784092825!2d79.10145254589841!3d12.009924873581818!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2sin!4v1448883859107"
                height="450" frameborder="0" allowfullscreen="">
        </iframe>
    </section>
</main>
